<p style="color: grey"><%= notice %></p>
<div class="container">
  <h1>Movies</h1>
  <%= link_to "New movie", new_movie_path, class: "btn btn-success" %>

  <div id="movies">
    <div class="row">
      <% @movies.each do |movie| %>
        <%= render movie %>
      <% end %>
    </div>
  </div>
</div>


